define nic_dmz = 10.0.0.1
define nic_inet = 192.168.1.1
define guarded_ports = 53
table inet filter {
    chain global {
        ct state established,related accept
        ct state invalid drop
        ip protocol icmp accept
        ip6 nexthdr icmpv6 accept
        udp dport 53 accept
    }
