# CORRECT â€” timeout is applied to the element, value follows after colon
add element ip Tip Sip { 192.168.1.10 expires 1h }
add element ip6 Tip6 Sip6  { 192.168.1.10 }
add element inet Tinet Sinet { 10.0.0.1 timeout 1h : continue }
add element netdev Tnetdev Snetdev { 10.0.0.1 timeout 1h : return }
add element bridge Tbridge Sbridge { 10.0.0.1 timeout 1h : accept }
add element arp Tarp Sarp { 10.0.0.1 timeout 1h : accept }
add element ip Tip Sip { 10.0.0.1 timeout 1h : drop }
add element ip Tip Sip { 10.0.0.1 timeout 1h : return }
add element ip Tip Sip { 10.0.0.1 timeout 1h : accept }
add element ip Tip Sip { 10.0.0.1 timeout 1h : drop }

